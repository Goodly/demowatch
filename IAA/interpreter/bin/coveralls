#!/home/ericwimsatt/IAA-image/interpreter/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'coveralls==0.5','console_scripts','coveralls'
__requires__ = 'coveralls==0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('coveralls==0.5', 'console_scripts', 'coveralls')()
    )
